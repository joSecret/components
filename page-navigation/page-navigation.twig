{#
/**
 * @file
 * Template for a Page Navigation component.
 *
#}
{% apply spaceless %}

{%
  embed 'versatile:navbar' with {
    container: 'fixed',
    navbar_theme: 'light',
    navbar_utility_classes: [
      'justify-content-between',
    ],
  }
%}
  {% block branding %}
    {{ page.branding }}
  {% endblock %}

  {% block main %}
    {{ page.nav_main }}
  {% endblock %}

  {% block additional %}
    {{ page.nav_additional }}
  {% endblock %}
{% endembed %}

{% endapply %}
